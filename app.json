{
  "expo": {
    "name": "Tomato Advisor",
    "slug": "TomatoAdvisor",
    "version": "1.1.0",
    "web": {
      "favicon": "./assets/icon.jpg"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app accesses your photos to let you choose a profile picture."
        }
      ],
      "expo-sqlite"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.christopher_kelvin.myexpoapp"
    },
    "extra": {
      "eas": {
        "projectId": "40a832f3-aecc-48eb-a14a-a902d4ade4f0"
      }
    }
  }
}
